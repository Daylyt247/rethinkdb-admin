{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "jsx": "react",
    "target": "ES5",
    "outDir": "./build",
    "baseUrl": "./",
    "incremental": true,
    "noImplicitAny": true,
    "allowJs": true
  },
  "exclude": [
    "node_modules",
    "build"
  ]
}
